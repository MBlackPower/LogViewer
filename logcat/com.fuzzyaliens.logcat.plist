<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.fuzzyaliens.logcat</string>
	<key>ServiceIPC</key>
	<true/>
	<key>Sockets</key>
	<dict>
		<key>Listener</key>
		<dict>
			<key>SockServiceName</key>
			<string>1001</string>
		</dict>
	</dict>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/logcat</string>
	</array>
</dict>
</plist>
